# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.getIndex(id: Long ?= 0)
POST    /                           controllers.Application.postIndex()

# welcome
# test
GET     /welcome                         controllers.Application.welcome()

# test
GET     /test                            controllers.Application.test()
GET     /home                           controllers.Application.home()
GET     /code                           controllers.Application.code()

GET     /test5                           controllers.Application.test5()
GET     /data                            controllers.Application.data()

GET     /m1                           controllers.Application.m1()
GET     /fileUpload                   controllers.Application.fileUpload()
GET     /m3                           controllers.Application.m3()



GET     /VWAP                            controllers.Application.VWAP()
GET     /TWAP                            controllers.Application.TWAP()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /webjars/*file              controllers.WebJarAssets.at(file)





GET     /hello                      controllers.Application.sayHello()



POST  /upload         				 controllers.Application.upload()